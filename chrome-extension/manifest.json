{
    "manifest_version": 2,

    "name": "Knapsack",
    "description": "Send page to Knapsack to be archived for offline reading.",
    "version": "1.0.2",

    "permissions": [
        "tabs",

        "identity",
        "identity.email",

        "https://accounts.google.com/*",
        "https://www.googleapis.com/*",

        "http://localhost:6543/",
        "*://*.quuux.org/"
    ],

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "oauth2": {
        "client_id": "843379878054-8m477vs3othq00e1d27kpcrkhv1i8o92.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/userinfo.email"
        ]
    }
}